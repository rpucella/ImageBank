{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
    "applicationName": "ImageBank",
    "applicationId": "github.com/rpucella/imagebank",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources",
  "url": "/",
  "enableServer": true,
  "enableExtensions": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "window.*",
    "extensions.*"
  ],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "ImageBank",
      "width": 1600,
      "height": 1200,
      "minWidth": 1200,
      "minHeight": 800,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "_icon": "/resources/icons/appIcon.png",
      "_icon": "/dist/assets/add.svg",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    },
    "browser": {
      "globalVariables": {},
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
        "filesystem.*",
        "os.*"
      ]
    }
  },
  "cli": {
    "binaryName": "imagebank",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "__clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.3.0",
    "clientVersion": "6.3.0"
  },
  "extensions": [
    {
      "id": "imagebank_backend",
      "command": "node ${NL_PATH}/server/server-neutralino.js",
      "__command": "${NL_PATH}/../MacOS/backend"
    }
  ],
  "buildScript": {
    "mac": {
      "architecture": [
        "arm64"
      ],
      "minimumOS": "10.13.0",
      "appName": "ImageBank",
      "appBundleName": "ImageBank",
      "appIdentifier": "github.com/rpucella/imagebank",
      "appIcon": "resources/icons/appIcon.png"
    }
  }
}
