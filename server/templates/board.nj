{% extends "layout.nj" %}


{% block container %}

  <h1>{{pagetitle}}</h1>

  {{ pager(page, total, base) }}
  
  <div class="board-grid">
  {% for img in images %}
  <a class="board-cell" href="/image/{{img['uuid']}}">
    <img src="{{img['link']}}" width="100%">
  </a>
  {% endfor %}
  </div>

  {{ pager(page, total, base) }}
  
{% endblock %}
    
